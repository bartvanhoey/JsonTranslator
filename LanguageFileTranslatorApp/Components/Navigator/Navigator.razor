<div class="btn-toolbar" role="toolbar" aria-label="Toolbar with button groups">
    <div class="btn-group mr-2" role="group" aria-label="First group">
        <button type="button" class="btn btn-secondary" @onclick="GotoFirstByKey"><i class="bi bi-chevron-bar-left"></i></button>
        <button type="button" class="btn btn-secondary" @onclick="GotoPreviousByKey"><i class="bi bi-chevron-left"></i></button>
        <button type="button" class="btn btn-secondary">Key</button>
        <button type="button" class="btn btn-secondary" @onclick="GotoNextByKey"><i class="bi bi-chevron-right"></i></button>
        <button type="button" class="btn btn-secondary" @onclick="GotoLastByKey"><i class="bi bi-chevron-bar-right"></i></button>
    </div>
    <div class="btn-group mr-2" role="group" aria-label="Second group">
        <button type="button" class="btn btn-secondary"  @onclick="GotoFirstById"><i class="bi bi-chevron-bar-left"></i></button>
        <button type="button" class="btn btn-secondary" @onclick="GotoPreviousById"><i class="bi bi-chevron-left"></i></button>
        <button type="button" class="btn btn-secondary">Id</button>
        <button type="button" class="btn btn-secondary" @onclick="GotoNextById"><i class="bi bi-chevron-right"></i></button>
        <button type="button" class="btn btn-secondary" @onclick="GotoLastById"><i class="bi bi-chevron-bar-right"></i></button>
    </div>
</div>
